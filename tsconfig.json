{
  "compilerOptions": {
    "incremental": true,
    "target": "es5",
    "module": "esnext",
    "lib": ["DOM", "ESNext"],
    "sourceMap": true,
    "importHelpers": true,
    "declaration": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "allowJs": false,
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "suppressImplicitAnyIndexErrors": true,
    "allowSyntheticDefaultImports": false,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "rootDir": ".",
    "baseUrl": ".",
    "paths": {
      "@/tkeel-console-business-components/*": [
        "./packages/tkeel-console-business-components/src/*"
      ],
      "@/tkeel-console-components/*": [
        "./packages/tkeel-console-components/src/*"
      ],
      "@/tkeel-console-constants/*": [
        "./packages/tkeel-console-constants/src/*"
      ],
      "@/tkeel-console-hooks/*": ["./packages/tkeel-console-hooks/src/*"],
      "@/tkeel-console-icons/*": ["./packages/tkeel-console-icons/src/*"],
      "@/tkeel-console-plugin-admin-plugins/*": [
        "./packages/tkeel-console-plugin-admin-plugins/src/*"
      ],
      "@/tkeel-console-plugin-admin-tenants/*": [
        "./packages/tkeel-console-plugin-admin-tenants/src/*"
      ],
      "@/tkeel-console-plugin-example/*": [
        "./packages/tkeel-console-plugin-example/src/*"
      ],
      "@/tkeel-console-portal-admin/*": [
        "./packages/tkeel-console-portal-admin/src/*"
      ],
      "@/tkeel-console-portal-base/*": [
        "./packages/tkeel-console-portal-base/src/*"
      ],
      "@/tkeel-console-portal-tenant/*": [
        "./packages/tkeel-console-portal-tenant/src/*"
      ],
      "@/tkeel-console-themes/*": ["./packages/tkeel-console-themes/src/*"],
      "@/tkeel-console-types/*": ["./packages/tkeel-console-types/src/*"],
      "@/tkeel-console-utils/*": ["./packages/tkeel-console-utils/src/*"]
    },
    "plugins": [{ "name": "typescript-plugin-css-modules" }]
  },
  "include": ["./packages/*/src/"],
  "exclude": ["**/node_modules/", "**/dist/", "**/scripts/"],
  "references": [
    {
      "path": "./packages/tkeel-console-business-components/"
    },
    {
      "path": "./packages/tkeel-console-components/"
    },
    {
      "path": "./packages/tkeel-console-constants/"
    },
    {
      "path": "./packages/tkeel-console-hooks/"
    },
    {
      "path": "./packages/tkeel-console-icons/"
    },
    {
      "path": "./packages/tkeel-console-plugin-admin-plugins/"
    },
    {
      "path": "./packages/tkeel-console-plugin-admin-tenants/"
    },
    {
      "path": "./packages/tkeel-console-plugin-example/"
    },
    {
      "path": "./packages/tkeel-console-portal-admin/"
    },
    {
      "path": "./packages/tkeel-console-portal-base/"
    },
    {
      "path": "./packages/tkeel-console-portal-tenant/"
    },
    {
      "path": "./packages/tkeel-console-themes/"
    },
    {
      "path": "./packages/tkeel-console-types/"
    },
    {
      "path": "./packages/tkeel-console-utils/"
    }
  ]
}
